{
  "name": "__MSG_extName__",
  "version": "1.0.6",
  "manifest_version": 2,
  "homepage_url": "http://momane.com/",
  "permissions": [
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://rarbg.to/torrents.php*"
      ],
      "js": [
        "lib/jquery-3.1.0.min.js",
        "main.js"
      ],
      "css": [
        "main.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "logo.png",
    "16": "logo.png"
  },
  "web_accessible_resources": [
    "*.png"
  ],
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "__MSG_extName__"
  }
}